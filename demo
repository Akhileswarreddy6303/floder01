package com.demo;

public class Demo 
{
	public static void main(String[] args) 
	{
		String n="Akhil";
		System.out.println("Hello");
		int cd=1;
		System.out.println(cd);
		System.out.println(n);
		var b='D';
		System.out.println(b);
		double f=2.345;
		System.out.println(f);
		cd=23;
		System.out.println(cd++);//23
		System.out.println(cd);//24
		System.out.println(++cd);//25
		System.out.println(" ");
		System.out.println(cd--);//25
		System.out.println(cd); //24
	}
}
